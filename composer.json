{
  "name": "macmotp/codegen-laravel",
  "version": "v0.3.0",
  "description": "Generate human friendly codes - Laravel Extension",
  "keywords": [
    "macmotp",
    "codegen",
    "code",
    "code generator",
    "human friendly code",
    "reference code",
    "semantic code",
    "receipt numbers",
    "laravel"
  ],
  "homepage": "https://github.com/macmotp/codegen-laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Marco Gava",
      "email": "marco.gava@me.com",
      "homepage": "https://github.com/macmotp",
      "role": "Software Engineer"
    }
  ],
  "require": {
    "php": ">=8.1",
    "macmotp/codegen": "^0.2.0",
    "spatie/laravel-package-tools": "^1.15"
  },
  "require-dev": {
    "orchestra/testbench": "^v8.5",
    "phpunit/phpunit": "^10.2"
  },
  "autoload": {
    "psr-4": {
      "Macmotp\\": "src/CodegenLaravel"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Macmotp\\CodegenLaravel\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --no-coverage",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
