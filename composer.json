{
  "name": "macmotp/codegen-laravel",
  "version": "v0.1.4",
  "description": "Generate human friendly codes - Laravel Extension",
  "keywords": [
    "macmotp",
    "codegen",
    "code",
    "code generator",
    "human friendly code",
    "reference code",
    "semantic code",
    "receipt numbers",
    "laravel"
  ],
  "homepage": "https://github.com/macmotp/codegen-laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Marco Gava",
      "email": "marco.gava@me.com",
      "homepage": "https://github.com/macmotp",
      "role": "Software Engineer"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/contracts": "^8.37",
    "macmotp/codegen": "^0.1.5",
    "spatie/laravel-package-tools": "^1.4.3"
  },
  "require-dev": {
    "brianium/paratest": "^6.2",
    "nunomaduro/collision": "^5.3",
    "orchestra/testbench": "^6.15",
    "phpunit/phpunit": "^9.3",
    "spatie/laravel-ray": "^1.9",
    "vimeo/psalm": "^4.4"
  },
  "autoload": {
    "psr-4": {
      "Macmotp\\": "src/CodegenLaravel"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Macmotp\\CodegenLaravel\\Tests\\": "tests"
    }
  },
  "scripts": {
    "psalm": "vendor/bin/psalm",
    "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
